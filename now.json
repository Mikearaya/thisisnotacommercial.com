{
  "version": 2,
  "builds": [
    { "src": "frontend/next.config.js", "use": "@now/next" },
    { "src": "controlpanel/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/controlpanel/(.*)", "dest": "/controlpanel/$1" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ],
  "regions": ["cdg1"]
}
